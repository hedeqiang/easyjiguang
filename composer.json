{
    "name": "hedeqiang/easyjiguang",
    "description": "极光 API Server SDK for PHP",
    "keywords": [
        "jiguang",
        "push",
        "verify",
        "sms",
        "link"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "hedeqiang",
            "email": "laravel_code@163.com"
        },{
            "name": "houseme",
            "email": "houseme@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "pimple/pimple": "^3.0",
        "monolog/monolog": "^1.22 || ^2.0",
        "symfony/http-foundation": "^2.7 || ^3.0 || ^4.0 || ^5.0",
        "ext-openssl": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*"
    },
    "autoload": {
        "psr-4": {
            "EasyJiGuang\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EasyJiGuang\\Providers\\JiGuangServiceProvider"
            ],
            "aliases": {
                "EasyJiGuang": "EasyJiGuang\\Facades\\EasyJiGuang"
            }
        }
    }
}
